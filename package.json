{
  "name": "angular-multipage-demo-with-oidc",
  "version": "12.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=false  --port=4200 --proxy-config proxy/local.config.json --open",
    "start:dev": "ng serve --ssl=true  --port=4200 --proxy-config proxy/dev.config.json --open",
    "start:int": "ng serve --ssl=true  --port=4200 --proxy-config proxy/int.config.json --open",
    "start:prod": "ng serve --ssl=true  --port=4200 --proxy-config proxy/prod.config.json --open",
    "build": "ng build",
    "build:dev": "ng build --prod --configuration=dev",
    "build:int": "ng build --prod --configuration=int",
    "build:prod": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "code-format": "npx prettier --write \"src/**/*.{ts,html,md,scss,json}\" ",
    "code-format:test": "npx prettier --check \"src/**/*.{ts,html,md,scss,json}\" "
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "angular-oauth2-oidc": "^12.1.0",
    "bootstrap": "^5.1.2",
    "bootstrap-icons": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
