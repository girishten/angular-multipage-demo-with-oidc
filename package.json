{
  "name": "angular-multipage-demo-with-oidc",
  "version": "13.0.0",
  "description": "Angular Dashboard Application with OAuth 2.0 OIDC, Public & Private Master Routes and Some Other Helpers.",
  "repository": "https://github.com/girishten/angular-multipage-demo-with-oidc.git",
  "author": "Girish S <girishten@live.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true  --port=4200 --proxy-config proxy/local.config.json --open",
    "start:dev": "ng serve --ssl=true  --port=4200 --proxy-config proxy/dev.config.json --open",
    "start:int": "ng serve --ssl=true  --port=4200 --proxy-config proxy/int.config.json --open",
    "start:prod": "ng serve --ssl=true  --port=4200 --proxy-config proxy/prod.config.json --open",
    "build": "ng build",
    "build:dev": "ng build --prod --configuration=development",
    "build:int": "ng build --prod --configuration=staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "code-format": "npx prettier --write \"src/**/*.{ts,html,md,scss,json}\" ",
    "code-format:test": "npx prettier --check \"src/**/*.{ts,html,md,scss,json}\" ",
    "gen:doc": "npx compodoc -p tsconfig.doc.json -n \"Documentation\""
  },
  "private": true,
  "keywords": [
    "Angular",
    "Dashboard",
    "OAuth 2.0",
    "OIDC",
    "Multiple Master Page",
    "Architechure"
  ],
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
